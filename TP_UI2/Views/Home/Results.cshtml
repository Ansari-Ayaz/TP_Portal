<div class="container">
    <div class="form">
        <div class="form-body">
            <div class="row">
                <div class="col-md-12">
                    <table id="tblResult" class="table table-bordered table-striped table-white">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Test Name</th>
                                <th>Start Time</th>
                                <th>End Time</th>
                                <th>Submission Time</th>
                                <th>Number Of Correct</th>
                                <th>Number Of In Correct</th>
                                <th>Points</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts{
<script>
    $(document).ready(function(){
        GetAllResults();
    })
    function GetAllResults(){
        $("#tblResult").dataTable({
        "ajax":{
            url:baseUrl + "tp/GetAllCandResults",
            type:"GET",
            contentType:"application/json",
            dataType:"JSON",
            dataSrc:"",
            async:false
            },
        "columns":[
        {"data":"name"},
        {"data":"testName"},
        {   
            "data":null,
            "render":function(a,b,c){
                return moment(c.startTime).format("DD-MM-YYYY, hh:mm:ss")
            }
        },
        {
            "data":null,
            "render":function(a,b,c){
                return moment(c.endTime).format("DD-MM-YYYY, hh:mm:ss")
            }
        },
        {
            "data":null,
            "render":function(a,b,c){
                return moment(c.submissionDate).format("DD-MM-YYYY, hh:mm:ss")
            }
        },
        {"data":"correctCount"},
        {"data":"incorrectCount"},
        {"data":"points"}
        ]
       })
    }

</script>

}
