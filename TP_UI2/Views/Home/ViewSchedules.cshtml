@{
}
<div class="card">
    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <table id="tblSche" class="table table-striped table-hover table-bordered">
                    <thead>
                        <tr>
                            <th>Test Name</th>
                            <th>Schedule Start Time</th>
                            <th>Schedule End Time</th>
                        </tr>
                    </thead>
                    <tbody>
                        
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@section Scripts{
    <script>
    $('#tblSche').dataTable({
        "ajax":{
            url:baseUrl + "tp/GetAllSchedules",
            type:"GET",
            dataType:"JSON",
            contentType:"application/json",
            async:false,
            dataSrc:""
        },
        "columns":[
        {"data":"testName"},
        {
            "data":null,
            "render":function(a,b,c){
                return moment(c.startTime).format("DD-MM-YYYY, hh:mm:ss")
            }
        },
        {
            "data":null,
            "render":function(a,b,c){
                return moment(c.endTime).format("DD-MM-YYYY, hh:mm:ss")
            }
        }
        ]
    })
    </script>
}